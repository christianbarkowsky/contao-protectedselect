{
    "name":"christianbarkowsky/contao-protectedselect",
    "description":"Adds a new form select widget which hides the internal field values in the frontend",
    "keywords":["contao", "form", "field", "selectmenu"],
    "type":"contao-bundle",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"Plenta",
            "homepage":"https://plenta.io",
            "role":"Developer"
        }
    ],
    "homepage": "https://github.com/christianbarkowsky/contao-protectedselect",
    "support":{
            "issues":"https://github.com/christianbarkowsky/contao-protectedselect/issues",
            "source":"https://github.com/christianbarkowsky/contao-protectedselect"
    },
    "require":{
            "php":">=7.4",
            "contao/core-bundle": "~4.9"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "contao/manager-bundle": "~4.9"
    },
    "extra": {
        "contao-manager-plugin": "Plenta\\ProtectedSelect\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Plenta\\ProtectedSelect\\": "src/Plenta/ProtectedSelect"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "contao-components/installer": true,
            "contao/manager-plugin": true,
            "contao-community-alliance/composer-plugin": true
        }
    },
    "replace": {
        "contao-legacy/contao-protectedselect": "self.version"
    }
}
